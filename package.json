{
  "name": "wartremover",
  "version": "1.3.0",
  "description": "stream transform to turn bunyan json log entries into plaintext",
  "keywords": [
    "bunyan"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "homepage": "https://github.com/robey/node-wartremover",
  "repository": {
    "type": "git",
    "url": "git://github.com/robey/node-wartremover.git"
  },
  "licenses": [
    {
      "type": "Apache License 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.txt"
    }
  ],
  "dependencies": {
    "clicolor": "*",
    "sprintf": "*"
  },
  "devDependencies": {
    "babel": "*",
    "bunyan": "*",
    "mocha": "*",
    "should": "*",
    "source-map-support": "*"
  },
  "main": "./lib/wartremover",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run prepublish",
    "prepublish": "babel --source-maps --out-dir lib src && babel --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib"
  }
}
